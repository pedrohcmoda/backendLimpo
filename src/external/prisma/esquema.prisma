datasource db {
    provider = "postgresql"
    url = env("DATABASE_URL")
}

generator client{
    provider = "prisma-client-js"
}

model usuario{
    id Int @id @default(autoincrement())
    email String @unique
    nome String
    senha String
    criadoEm DateTime @default(now())
    atualizadoEm DateTime @updatedAt
}

